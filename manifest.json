{
  "name": "Batch Image Downloader",
  "description": "Download every image on a web page",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "popup/index.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://www.google.ca/*"],
      "js": ["content.js"]
    }
  ],
  "manifest_version": 2
}
